eElement(\"li\");\r\n            let anchor = document.createElement(\"a\");\r\n            anchor.target = \"_blanc\"\r\n            anchor.href = a.link;\r\n            anchor.textContent = a.name;\r\n            addon.append(anchor);\r\n            addons.append(addon);\r\n        });\r\n        addonContainer.append(addons)\r\n        element.append(heading2, addonContainer)\r\n        return element\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"59aff3859dcc0cba3d6380e891923bca","cacheData":{"env":{}}}